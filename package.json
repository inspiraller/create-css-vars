{
  "name": "@inspiraller/create-css-vars",
  "version": "2.0.94",
  "description": "This converts a directory of css files into a file of variable names for use with React = styled components",
  "keywords": [
    "convert-css-to-variables",
    "css",
    "styled-components",
    "react",
    "typescript"
  ],
  "author": "Steve Tomlin",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/inspiraller/create-css-vars"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "create-css-vars": "bin/create-css-vars"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "start": "cross-env-shell babel-node --extensions '.ts,.tsx' src/index.ts --from=./css to=./ assets=/myassets",
    "prod:clear": "rimraf dist && mkdir dist",
    "prod:cleanTs": "rimraf dist/**/*.ts && rimraf dist/*.ts",
    "fixTsPaths": "cross-env-shell babel-node --extensions '.ts,.tsx' ci/fixTsPaths.ts",
    "prod:resolve": "tspath -f && mv -f dist/src/* dist && rm -r dist/src && npm run fixTsPaths",
    "build": "npm run prod:clear && npm run prod:cleanTs && tsc && npm run prod:resolve"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.11.2",
    "@types/jest": "^23.3.1",
    "@types/node": "^14.0.27",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-redom-jsx": "^3.0.1",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.25",
    "jest": "^26.4.0",
    "jest-sonar": "^0.2.10",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "seamless-immutable": "^7.1.4",
    "sonarqube-scanner": "^2.7.0",
    "ts-jest": "^26.2.0",
    "tspath": "^1.3.7",
    "typescript": "^4.1.3"
  }
}
